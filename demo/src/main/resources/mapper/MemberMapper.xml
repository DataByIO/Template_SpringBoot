<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.project.backend.SecurityService.Service.MemberMapper">
    <select id="userInfo" parameterType="Member" resultType="HashMap">
        SELECT *
        FROM members
        WHERE email = #{email}
    </select>
</mapper>

